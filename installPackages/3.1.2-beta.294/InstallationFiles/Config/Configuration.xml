<?xml version="1.0" encoding="utf-8"?>
<Configuration>
  <General>
    <LDAPPath></LDAPPath>
    <SMTPServer></SMTPServer>
    <Logging>True</Logging>
    <DBServer>%CMPortalSQLServer%</DBServer>
    <DataBase>%CMPortalDatabaseName%</DataBase>
    <AdminEmail></AdminEmail>
    <Culture>en-US</Culture>
    <LicenseKey>%CMPortalLicenseKey%</LicenseKey>
    <DBUser>%CMPDBUser%</DBUser>
    <DBPassword>%CMPDBPassword%</DBPassword>
    <AuthenticationMode>%CMPAuthMode%</AuthenticationMode>
    <AllowCachedPages>True</AllowCachedPages>
    <UseWMIConnectionPool>True</UseWMIConnectionPool>
    <AllowADFullNameLookup>True</AllowADFullNameLookup>
    <UseInstallNow>False</UseInstallNow>
    <AllowSecurityCaching>True</AllowSecurityCaching>
    <CustomerKey>%CustomerKey%</CustomerKey>
    <TreadNotification>Detailed</TreadNotification>
  </General>
  <Service>
    <AutoFillMDTBIOSGUID>False</AutoFillMDTBIOSGUID>
    <ClearMDTMACOnBIOSMerge>False</ClearMDTMACOnBIOSMerge>
  </Service>
  <Collection>
    <ADGroupQueryIntervalType>hours</ADGroupQueryIntervalType>
    <ADGroupQueryIntervalValue>24</ADGroupQueryIntervalValue>
    <UseWQLQueries>False</UseWQLQueries>
  </Collection>
  <ConfigMgr>
    <Server>%CMSiteServer%</Server>
    <NameSpacePath></NameSpacePath>
    <ReportingURL></ReportingURL>
    <DBServer>%CMSiteSQLServer%</DBServer>
    <DataBase>%CMSiteDatabaseName%</DataBase>
    <DBUser>%ConfigMgrDbUser%</DBUser>
    <DBPassword>%ConfigMgrDbPassword%</DBPassword>
    <DisabledSiteServers></DisabledSiteServers>
    <AuthenticationMode>%ConfigMgrAuthMode%</AuthenticationMode>
    <RerunMethod>Collection</RerunMethod>
  </ConfigMgr>
  <MDT>
    <DBServer>%MdtSqlServer%</DBServer>
    <DataBase>%MdtSqlDatabase%</DataBase>
    <ComputerBackupLocation>NONE</ComputerBackupLocation>
    <BackupShare>
    </BackupShare>
    <BackupDirectory>%ComputerName%</BackupDirectory>
    <AllowInstall>True</AllowInstall>
    <EnableUSMT>True</EnableUSMT>
    <AuthenticationMode>%MDTAuthMode%</AuthenticationMode>
    <DBUser>%MdtDbUser%</DBUser>
    <DBPassword>%MdtDbPassword%</DBPassword>
    <DeleteFromMDT>True</DeleteFromMDT>
    <DeleteFromConfigMgr>True</DeleteFromConfigMgr>
    <DeleteFromAD>False</DeleteFromAD>
    <DeleteFromDNS>False</DeleteFromDNS>
    <Enabled>%EnableMdtIntegration%</Enabled>
  </MDT>
  <Program>
    <InstallProgramName>Install</InstallProgramName>
    <InstallCommandLine>wrapper.exe /Install</InstallCommandLine>
    <InstallProgramFlags>2282790913</InstallProgramFlags>
    <ReinstallProgramName>Reinstall</ReinstallProgramName>
    <ReinstallCommandLine>wrapper.exe /Reinstall</ReinstallCommandLine>
    <ReinstallProgramFlags>2282790912</ReinstallProgramFlags>
    <UninstallProgramName>Uninstall</UninstallProgramName>
    <UninstallCommandLine>wrapper.exe /Uninstall</UninstallCommandLine>
    <UninstallProgramFlags>2282790912</UninstallProgramFlags>
    <RepairProgramName>Repair</RepairProgramName>
    <RepairCommandLine>wrapper.exe /Repair</RepairCommandLine>
    <RepairProgramFlags>2282790912</RepairProgramFlags>
    <PatchProgramName>Patch</PatchProgramName>
    <PatchCommandLine>wrapper.exe /Install</PatchCommandLine>
    <PatchProgramFlags>2282790912</PatchProgramFlags>
    <CustomCommandLine>wrapper.exe /</CustomCommandLine>
    <CustomProgramFlags>2282790913</CustomProgramFlags>
    <VirtualProgramFlags>0</VirtualProgramFlags>
  </Program>
  <TaskSequence>
    <TaskSequencePrefix>
    </TaskSequencePrefix>
  </TaskSequence>
  <Advertisement>
    <MandatoryAssignment>True</MandatoryAssignment>
  </Advertisement>
  <Folders>
    <Platforms>x86,x64</Platforms>
    <Bits>32,64</Bits>
    <DefaultLanguage>ENUS</DefaultLanguage>
    <DefaultBit>32</DefaultBit>
    <DefaultPlatform>All</DefaultPlatform>
    <PackageTypes>Normal</PackageTypes>
    <DefaultPackageType>Normal</DefaultPackageType>
  </Folders>
  <Package>
    <PackageRootFolder></PackageRootFolder>
    <PackageFlags>0</PackageFlags>
    <UseCommonPackageShare>True</UseCommonPackageShare>
    <ImportGUID>True</ImportGUID>
    <ApplicationRootFolder></ApplicationRootFolder>
    <LimitingDeviceCollectionName>All Systems</LimitingDeviceCollectionName>
    <LimitingDeviceCollectionID>SMS00001</LimitingDeviceCollectionID>
    <LimitingUserCollectionName></LimitingUserCollectionName>
  </Package>
  <Security>
    <ServerOperatorGroup>Administrators</ServerOperatorGroup>
    <ManualSoftwareCreatorGroup>
    </ManualSoftwareCreatorGroup>
    <SystemAdministratorGroup>%SysAdminGroup%</SystemAdministratorGroup>
    <Administrators>%SysAdminGroup%</Administrators>
  </Security>
  <SoftwareOrder>
    <Currency>$</Currency>
    <AdminEmail>
    </AdminEmail>
    <SystemEmail>
    </SystemEmail>
    <UseMail>False</UseMail>
    <RequestMailSubject>New Software Order Request</RequestMailSubject>
    <RequestMailUrl>
    </RequestMailUrl>
    <StatusMailSubject>
    </StatusMailSubject>
    <StatusMailUrl>
    </StatusMailUrl>
    <Categories>
    </Categories>
    <CurrencyIsPrefixed>True</CurrencyIsPrefixed>
  </SoftwareOrder>
  <Software>
    <DefaultImportCategories>1</DefaultImportCategories>
    <DefaultImportOSTypes>
    </DefaultImportOSTypes>
  </Software>
  <Distribution>
    <KeepSelections>True</KeepSelections>
  </Distribution>
  <Commence>
    <NamingMethod>PrefixMAC</NamingMethod>
    <NamingPrefix>PXE</NamingPrefix>
  </Commence>
  <DNS>
    <DeleteOnComputerDelete>False</DeleteOnComputerDelete>
    <DeleteOnInstall>False</DeleteOnInstall>
    <DeleteOnReinstall>False</DeleteOnReinstall>
    <DeleteOnDemand>True</DeleteOnDemand>
  </DNS>
  <Reporting>
    <DBServer>%SSRSServer%</DBServer>
    <DataBase>%SSRSDBName%</DataBase>
    <AuthenticationMode>%SSRSAuth%</AuthenticationMode>
    <DBUser>%SSRSLogin%</DBUser>
    <DBPassword>%SSRSPassword%</DBPassword>
  </Reporting>
</Configuration>